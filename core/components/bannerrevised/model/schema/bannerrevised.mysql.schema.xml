			<column key="name" length="" collation="A" null="false" />
		</index>

		<composite alias="Ads" class="brevAdPosition" local="id" foreign="position" cardinality="many" owner="local" />
		<composite alias="Clicks" class="brevClick" local="id" foreign="position" cardinality="many" owner="local" />
