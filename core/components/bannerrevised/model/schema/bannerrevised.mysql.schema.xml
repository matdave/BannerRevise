		<composite alias="Clicks" class="brevClick" local="id" foreign="ad" cardinality="many" owner="local" />
	</object>
	<object class="brevPosition" table="ban_rev_positions">
		<field key="name" dbtype="varchar" precision="255" phptype="string" null="false" default=""/>

